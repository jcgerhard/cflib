{
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "template": "node_modules/clean-jsdoc-theme"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    }
}
